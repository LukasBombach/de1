{
  "name": "de1-debugger",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "dev": "concurrently \"BROWSER=none yarn dev:cra\" \"wait-on http://localhost:3000 && yarn dev:electron\"",
    "dev:electron": "electron .",
    "dev:cra": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "antd": "3.20.7",
    "de1": "0.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.11",
    "@rescripts/rescript-env": "0.0.10",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "concurrently": "4.1.1",
    "electron": "5.0.6",
    "electron-builder": "20.44.4",
    "electron-devtools-installer": "2.2.4",
    "electron-is-dev": "1.1.0",
    "native-ext-loader": "2.3.0",
    "node-loader": "0.6.0",
    "react-router-dom": "5.0.1",
    "typescript": "3.5.2",
    "wait-on": "3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
